                        });
                        google.maps.event.addListener(marker, 'click', (function (marker, content, info) {
                            return function () {
                                document.getElementById('emergency_' + marker.get("id")).click();
                                info.open(map, marker);
                            };
                        })(marker, info));
                    }
                }
            }
        });
    }
